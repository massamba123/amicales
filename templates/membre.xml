<odoo>
    <data>

        <template id="amicale_members_template" name="Amicale Members">
            <table>
                <thead>
                    <tr>
                        <th>Role</th>
                        <th>matricule</th>
                        <th>Commission</th>
                        <th>Amical</th>
                    </tr>
                </thead>
                <tbody>
                    {% for member in members %}
                        <tr>
                            <td>{{ member.role }}</td>
                            <td>{{ member.maticule }}</td>
                            <td>{{ member.commission_id.nomCommission }}</td>
                            <td>{{ member.amicale_id.nomAmicale }}</td>
                        </tr>
                    {% endfor %}
                </tbody>
            </table>
            <t t-call="amicales.footer"/>
        </template>
    </data>
</odoo>



